<section class="login">
  <h3>Login</h3>
  <%= form_for(:session, url: login_path) do |f| %>
    <div class="form-group">
      Email: <%= f.text_field :email %>
    </div>
    <div class="form-group">
      Password: <%= f.password_field :password %>
    </div>
    <%= f.submit "Submit" %>
  <% end %>
</section>
